<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="/style.css">
    <title>Dinos Directory</title>
  </head>
  <body>
      <h1>Dinos</h1>
      {{#allDinos}}
      <ul>
        <li><img src="{{imageurl}}" alt="{{name}}"></li>
        <li>{{name}}</li>
        <ul>
          {{#color}}
          <li>{{.}}</li>
          {{/color}}
        </ul>
        <li>{{diet}}</li>
        <div class="buttons">
          <button type="button" id='editButton' name="button">Edit</button>
          <button type="button" id='deleteButton' name="button">Delete</button>
        </div>
      </ul>
      {{/allDinos}}
      <script src="/main.js"></script>
  </body>
</html>
